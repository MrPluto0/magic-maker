{
  "include": [
    "src/**/*",
    "./typed-router.d.ts",
    "./auto-imports.d.ts",
    "./components.d.ts"
  ],
  "compilerOptions": {
    "target": "ES2015",
    "types": ["vite/client"],
    // "typeRoots": ["node_modules", "./packages/types"],
    "module": "ES2022",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "paths": {
      "@/*": ["./src/*"],
      "@pkg/*": ["./packages/*"],
    },
  }
}